game:GetService("StarterGui"):SetCore("SendNotification",{ Title = "Galaxy Hub"; Text = "Made by Clover King#6209"; })
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Galaxy Sky Community", "GrapeTheme")

---Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Devs:")
local MainSection = Main:NewSection("Clover King#6209")
local MainSection = Main:NewSection("YSNOW#0285")

---Basic Scripts
local Basic = Window:NewTab("Basic Scripts")
local BasicSection = Basic:NewSection("Basic Scripts")
BasicSection:NewTextBox("WalkSpeed", "Type a Number", function(sp)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (sp)
end)
BasicSection:NewButton("Reset WalkSpeed", "Resets your WalkSpeed", function(rw)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)
BasicSection:NewTextBox("JumpPower", "Type a Number", function(jp)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = (jp)
end)
BasicSection:NewButton("Reset JumpPower", "Resets your JumpPower", function(rj)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 20
end)
BasicSection:NewTextBox("Teleport To Player", "Write the User of the Player", function(txt)
local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local p2 = "InsertPlayerHere" == (txt)

p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
end)
